<table>
  <thead>
    <tr>
			<th>Q</th>
			<th>Name</th>
			<th>Score</th>
    </tr>
  </thead>

  <tbody>
		<% 
			@top_phil.each do |value, keys|
		%>
      <tr>
				<td><%= raw keys.collect{|key|wikidata_entity_url(key)}.join('<br>') %></td>
				<td>
					<%=
						raw keys.collect{|key|
							begin
								do_english(key)
							rescue
								'â€¦'
							end
						}.join('<br>')
					%>
				</td>
				<td><%= sprintf('%.3f',value) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%# paginate @top_phil %> <%# page_entries_info @top_phil %>
